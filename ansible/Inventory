[managers]
manager ansible_host=10.154.0.15 ansible_python_interpreter=/usr/bin/python3

[managers:vars]
ansible_user=jenkins
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
ansible_ssh_private_key_file=~/.ssh/id_rsa

[workers]
worker-1 ansible_host=10.154.0.16 ansible_python_interpreter=/usr/bin/python3
worker-2 ansible_host=10.154.0.17 ansible_python_interpreter=/usr/bin/python3

[workers:vars]
ansible_user=jenkins
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
ansible_ssh_private_key_file=~/.ssh/id_rsa